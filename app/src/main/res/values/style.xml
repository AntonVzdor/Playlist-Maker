<?xml version="1.0" encoding="utf-8"?>
<!--suppress ALL -->
<resources>

    <style name="LinearMain">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/blue</item>
        <item name="android:orientation">vertical</item>
    </style>


    <style name="MainText">
        <item name="android:textSize">@dimen/textSizeMain</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/margin16</item>
        <item name="android:fontFamily">@font/ys_display</item>
        <item name="android:gravity">top</item>
        <item name="android:paddingTop">@dimen/paddingTop</item>
        <item name="android:paddingBottom">@dimen/paddingBottom</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ButtonMain">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginStart">@dimen/margin16</item>
        <item name="android:layout_marginEnd">@dimen/margin16</item>
        <item name="android:layout_marginBottom">@dimen/margin16</item>
        <item name="android:layout_weight">1</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:fontFamily">@font/ys_display</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/black_button</item>
        <item name="android:textIsSelectable">false</item>
        <item name="android:textSize">@dimen/textSizeMain</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="LinearSettings">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="background">@color/settings_color2</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="Toolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:layout_marginBottom">24dp</item>
    </style>

    <style name="SettingsText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/settings_color</item>
        <item name="android:textSize">@dimen/textSizeMain</item>
    </style>

    <style name="SwitchStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:paddingStart">@dimen/paddingStartSetting</item>
        <item name="android:paddingEnd">@dimen/paddingEndSetting</item>
        <item name="android:paddingTop">@dimen/paddingTopSetting</item>
        <item name="android:paddingBottom">@dimen/paddingBottomSetting</item>
        <item name="android:textColor">@color/settings_color</item>
        <item name="android:textSize">@dimen/textSizeSettings</item>
    </style>

    <style name="ButtonSettings">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingStart">@dimen/paddingStartSetting</item>
        <item name="android:paddingEnd">@dimen/paddingEndSetting</item>
        <item name="android:paddingTop">@dimen/paddingTopSetting</item>
        <item name="android:paddingBottom">@dimen/paddingBottomSetting</item>
        <item name="android:fontFamily">@font/ys_display</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/textSizeSettings</item>
        <item name="android:textColor">@color/settings_color</item>
    </style>

    <style name="SearchTextStyle">
        <item name="android:textColorHint">@color/YP_Text_Gray</item>
        <item name="android:drawableTint">@color/YP_Text_Gray</item>
        <item name="android:backgroundTint">@color/YP_Light_Gray</item>
    </style>

    <style name="IconSearchStyle">
        <item name="tint">@color/YP_Text_Gray</item>
        <item name="android:background">@color/YP_Light_Gray</item>
    </style>

    <style name="MessageTextStyle">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">56dp</item>
        <item name="paddingStart">12dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:textSize">@dimen/medium_text_size</item>
        <item name="android:textFontWeight">500</item>
    </style>

    <style name="ButtonMainStyle" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:padding">@dimen/main_button_padding</item>
        <item name="android:layout_marginVertical">4dp</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Medium</item>
        <item name="android:textColor">@color/YP_black</item>
        <item name="android:textSize">@dimen/medium_text_size</item>
        <item name="cornerRadius">@dimen/main_button_radius</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">@dimen/icon_padding</item>
    </style>

    <style name="FrameSetting">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:textColor">@color/black</item>
        <!--<item name="iconTint">@color/YP_black</item>-->
    </style>

    <style name="TextSettingsStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:textColor">@color/YP_black</item>
    </style>

    <style name="BackSettingsButtonStyle" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="iconGravity">textStart</item>

        <item name="iconTint">#1A1B22</item>
    </style>

    <style name="TextActivitySettings">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/settings_text_size</item>
        <item name="android:gravity">start</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="ImageSettingsStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">end</item>
        <item name="tint">@color/YP_Text_Gray</item>
    </style>

    <style name="SettingsActivityStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="BrandedSwitch">
        <item name="colorControlActivated">#AEAFB4</item>
        <item name="colorSwitchThumbNormal">#E6E8EB</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="thumbTint">@color/YP_Text_Gray</item>
        <item name="trackTint">@color/YP_Light_Gray</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:textColor">@color/YP_black</item>
    </style>

    <style name="SearchField">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">52dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@color/YP_Text_Gray</item>
        <item name="android:layout_marginHorizontal">16dp</item>
        <item name="android:paddingEnd">12dp</item>
    </style>



    <style name="EditTextStyle">
        <item name="android:textColorHint">@color/YP_Text_Gray</item>
    </style>
</resources>